{
  "name": "backend",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch --verbose",
    "build:ui": "rm -rf dist && cd ../admin && npm run build && cp -r dist ../backend/dist",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "lint": "eslint .",
    "start:test": "cross-env NODE_ENV=test node --watch index.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@types/jest": "^30.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "jest": "^30.2.0",
    "mockingoose": "^2.16.2",
    "supertest": "^7.1.4"
  }
}